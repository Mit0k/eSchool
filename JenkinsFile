pipeline{
    agent { label "Linux" }
    stages{

        stage('Deploy'){
        environment
            {
                SSH_HOST = credentials('ssh_host')
            }
            steps{
                script{
                    sh "scp -o StrictHostKeyChecking=no -i /home/azureuser/key.pem -v target/eschool.jar $SSH_HOST:/usr/local/share/eschool/"
                }
            }
        }
    }
}
 
 
